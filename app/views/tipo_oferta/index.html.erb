<h1 class="sub">Tipo de oferta</h1>

<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Tipo de oferta</th>
      <th colspan="3">Opciones</th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_oferta.each do |tipo_ofertum| %>
      <tr>
        <td><%= tipo_ofertum.nombre %></td>
        <td><%= link_to 'Detalles', tipo_ofertum %></td>
        <td><%= link_to 'Editar', edit_tipo_ofertum_path(tipo_ofertum) %></td>
        <td><%= link_to 'Eliminar', tipo_ofertum, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Agregar tipo de oferta', new_tipo_ofertum_path %>
