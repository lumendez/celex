<div id="filterrific_results">
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Paterno</th>
      <th>Materno</th>
      <th>Idioma</th>
      <th>Nivel asignado</th>
      <th>Examinador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @examen_colocacion_idiomas.each do |examen_colocacion_idioma| %>
      <tr>
        <td><%= examen_colocacion_idioma.nombres_upcase %></td>
        <td><%= examen_colocacion_idioma.paterno_upcase %></td>
        <td><%= examen_colocacion_idioma.materno_upcase %></td>
        <td><%= examen_colocacion_idioma.idioma %></td>
        <td><%= examen_colocacion_idioma.nivel_asignado %></td>
        <td><%= examen_colocacion_idioma.examinador %></td>
        <td><%= link_to 'Mostrar', examen_colocacion_idioma %></td>
        <td><%= link_to 'Editar', edit_examen_colocacion_idioma_path(examen_colocacion_idioma) %></td>
        <td><%= link_to 'Eliminar', examen_colocacion_idioma, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= paginate @examen_colocacion_idiomas, theme: 'twitter-bootstrap-3', param_name: :pagina %>
</div>
